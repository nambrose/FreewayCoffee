<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    
    android:orientation="vertical" >
    
   <LinearLayout 
    
    android:layout_height="40dp"
    android:layout_width="fill_parent"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:background="#00000000"
    android:layout_marginTop="5dp"
	android:layout_marginBottom="0dp"
    android:orientation="horizontal" >
            <com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_refund_but"
	  			 android:text="Refund"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:textColor="@color/White"
				android:onClick="DoMainListRefund"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton>
    

    <com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_noshow_but"
	  			 android:text="No Show"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:layout_marginLeft="5dp"
				android:textColor="@color/White"
				android:onClick="DoMainListNoShow"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton>
            
    <com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_delivered_but"
	  			 android:text="Deliver"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:layout_marginLeft="5dp"
				android:textColor="@color/White"
				android:onClick="DoMainListDelivered"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager2.FCOrderManagerNoParentPressButton>
            
    
            </LinearLayout>
</LinearLayout>
