<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    
    android:orientation="vertical" >
    
   <LinearLayout 
    
    android:layout_height="40dp"
    android:layout_width="700dp"
    android:layout_marginLeft="100dp"
    android:background="#00000000"
    android:layout_marginTop="5dp"
	android:layout_marginBottom="0dp"
    android:orientation="horizontal" >
            <com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_refund_but"
	  			 android:text="Refund"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:textColor="@color/White"
				android:onClick="DoMainListRefund"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton>
    

    <com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_noshow_but"
	  			 android:text="No Show"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:layout_marginLeft="5dp"
				android:textColor="@color/White"
				android:onClick="DoMainListNoShow"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton>
            
    <com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_delivered_but"
	  			 android:text="Delivered"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:layout_marginLeft="5dp"
				android:textColor="@color/White"
				android:onClick="DoMainListDelivered"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton>
            
    <com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton
            
            android:id="@+id/fc_om_order_main_list_child_header_in_process_but"
	  			 android:text="In Process"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:padding="1dp"
				android:textSize="15dp"
				android:layout_marginLeft="5dp"
				
				android:textColor="@color/White"
				android:onClick="DoMainListInProcess"
				android:layout_weight="1"
				android:layout_gravity="center"  android:background="@drawable/fc_om_main_list_child_but_bg">
        
            
            </com.freeewaycoffee.ordermanager.FCOrderManagerNoParentPressButton>
            </LinearLayout>
</LinearLayout>
